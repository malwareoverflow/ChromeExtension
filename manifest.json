{
  "manifest_version": 2,

  "name": "BPO River Homepage Changer",
  "description": "This extension will change the current homepage",
  "version": "1.0",

"chrome_settings_overrides": {
    "homepage": "http://www.bporiver.com"
  },


  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ]
}